    </EmbeddedResource>
    <EmbeddedResource Update="Views\EstudiosPage.xaml">
      <Generator>MSBuild:UpdateDesignTimeXaml</Generator>
    </EmbeddedResource>
    <EmbeddedResource Update="Views\PropuestasPage.xaml">
      <Generator>MSBuild:UpdateDesignTimeXaml</Generator>
    </EmbeddedResource>
    <EmbeddedResource Update="Views\SondePage.xaml">
      <Generator>MSBuild:UpdateDesignTimeXaml</Generator>
