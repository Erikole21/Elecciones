    <None Remove="Imagenes\Fajardo.jpg" />
    <None Remove="Imagenes\LLeras.jpg" />
    <None Remove="Imagenes\Marta.jpg" />
    <None Remove="Imagenes\Ordonez.jpg" />
    <None Remove="Imagenes\Petro.jpg" />
    <None Remove="Imagenes\Piedad.jpg" />
